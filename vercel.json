{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/taskulardo_frontend"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/taskulardo_frontend/$1"
      }
    ]
  }
  